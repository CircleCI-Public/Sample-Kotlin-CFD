<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/secondaryDark">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="124dp"
        android:layout_height="100dp"
        android:layout_marginBottom="35dp"
        android:elevation="2sp"
        android:translationZ="2sp"
        app:layout_constraintBottom_toBottomOf="@+id/soup_button"
        app:layout_constraintStart_toStartOf="@+id/soup_button"
        app:srcCompat="@drawable/soup" />

    <Button
        android:id="@+id/soup_button"
        android:layout_width="359dp"
        android:layout_height="103dp"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/pizza_button"
        tools:layout_editor_absoluteX="26dp" />

    <TextView
        android:id="@+id/item1_title5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Soup"
        android:textColor="@color/primaryDark"
        android:textSize="24sp"
        android:textStyle="bold"
        android:translationZ="2sp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/soup_button" />

    <TextView
        android:id="@+id/item1_title6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$4"
        android:textColor="@color/primaryDark"
        android:textSize="18sp"
        android:textStyle="bold"
        android:translationZ="2sp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/item1_title5" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:translationZ="2sp"
        app:layout_constraintBottom_toBottomOf="@+id/soup_button"
        app:layout_constraintEnd_toEndOf="@+id/soup_button"
        app:layout_constraintTop_toTopOf="@+id/soup_button"
        app:srcCompat="@drawable/chevron_right" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="124dp"
        android:layout_height="100dp"
        android:layout_marginBottom="35dp"
        android:elevation="2sp"
        android:translationZ="2sp"
        app:layout_constraintBottom_toBottomOf="@+id/pizza_button"
        app:layout_constraintStart_toStartOf="@+id/pizza_button"
        app:srcCompat="@drawable/pizza" />

    <Button
        android:id="@+id/pizza_button"
        android:layout_width="359dp"
        android:layout_height="103dp"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/hamburger_button"
        tools:layout_editor_absoluteX="26dp" />

    <TextView
        android:id="@+id/item1_title3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pizza"
        android:textColor="@color/primaryDark"
        android:textSize="24sp"
        android:textStyle="bold"
        android:translationZ="2sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        tools:layout_editor_absoluteY="234dp" />

    <TextView
        android:id="@+id/item1_title4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$8"
        android:textColor="@color/primaryDark"
        android:textSize="18sp"
        android:textStyle="bold"
        android:translationZ="2sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/item1_title3" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:translationZ="2sp"
        app:layout_constraintBottom_toBottomOf="@+id/pizza_button"
        app:layout_constraintEnd_toEndOf="@+id/pizza_button"
        app:layout_constraintTop_toTopOf="@+id/pizza_button"
        app:srcCompat="@drawable/chevron_right" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="124dp"
        android:layout_height="100dp"
        android:layout_marginBottom="35dp"
        android:elevation="2sp"
        android:translationZ="2sp"
        app:layout_constraintBottom_toTopOf="@+id/pizza_button"
        app:layout_constraintStart_toStartOf="@+id/hamburger_button"
        app:srcCompat="@drawable/hamburger" />

    <Button
        android:id="@+id/hamburger_button"
        android:layout_width="359dp"
        android:layout_height="103dp"
        android:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="123dp" />

    <ImageButton
        android:id="@+id/cart_button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="@color/primaryDark"
        android:tint="@color/primaryGreen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/cart" />

    <TextView
        android:id="@+id/item1_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Hamburger"
        android:textColor="@color/primaryDark"
        android:textSize="24sp"
        android:textStyle="bold"
        android:translationZ="2sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        tools:layout_editor_absoluteY="137dp" />

    <TextView
        android:id="@+id/item1_title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="$12"
        android:textColor="@color/primaryDark"
        android:textSize="18sp"
        android:textStyle="bold"
        android:translationZ="2sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/item1_title" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:translationZ="2sp"
        app:layout_constraintBottom_toBottomOf="@+id/hamburger_button"
        app:layout_constraintEnd_toEndOf="@+id/hamburger_button"
        app:layout_constraintTop_toTopOf="@+id/hamburger_button"
        app:layout_constraintVertical_bias="0.405"
        app:srcCompat="@drawable/chevron_right" />

</androidx.constraintlayout.widget.ConstraintLayout>